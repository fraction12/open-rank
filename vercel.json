{
  "framework": "astro",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "devCommand": "npm run dev",
  "redirects": [
    {
      "source": "/:path*",
      "has": [{ "type": "host", "value": "www.open-rank.com" }],
      "destination": "https://open-rank.com/:path*",
      "permanent": true
    }
  ]
}
