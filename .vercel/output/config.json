{
	"version": 3,
	"routes": [
		{
			"handle": "filesystem"
		},
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"src": "^/_server-islands/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/_image/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/leaderboard/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/leaderboard/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/puzzle/today/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/puzzle/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/submit/?$",
			"dest": "_render"
		},
		{
			"src": "^/archive/?$",
			"dest": "_render"
		},
		{
			"src": "^/docs/?$",
			"dest": "_render"
		},
		{
			"src": "^/leaderboard/?$",
			"dest": "_render"
		},
		{
			"src": "^/puzzle/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/$",
			"dest": "_render"
		}
	]
}